{
  "name": "eslint-config-agent",
  "version": "1.6.1",
  "description": "ESLint configuration package with TypeScript support",
  "main": "index.js",
  "type": "module",
  "exports": {
    ".": "./index.js",
    "./ddd": "./exports/ddd.js"
  },
  "files": [
    "index.js",
    "README.md",
    "CHANGELOG.md",
    "configs/",
    "plugins/",
    "exports/",
    "rules/**/index.js",
    "!rules/**/examples/",
    "!rules/**/*.spec.js",
    "!rules/**/*.test.js"
  ],
  "engines": {
    "node": ">=18.0.0"
  },
  "scripts": {
    "lint": "eslint .",
    "lint:test": "eslint test/",
    "test": "node scripts/test-runner.js",
    "test:old": "npm run test:valid && npm run test:invalid",
    "test:valid": "eslint test/valid.tsx test/preact-test.tsx test/typescript-rules.ts",
    "test:invalid": "eslint test/invalid.tsx test/long-function.tsx test/jsx-extension-test.js",
    "test:hooks": "eslint test/react-hooks-rules.tsx",
    "test:imports": "eslint test/import-export-rules.ts",
    "test:edge": "eslint test/edge-cases.tsx",
    "test:performance": "eslint test/performance-test.tsx",
    "test:comprehensive": "node scripts/test-runner.js",
    "test:ci": "eslint . --ignore-pattern 'test/**' --ignore-pattern 'scripts/**' --ignore-pattern 'rules/**/examples/**' --ignore-pattern 'configs/**' --ignore-pattern 'index.js' --max-warnings 0",
    "validate": "node scripts/validate-config.js",
    "release": "pnpm exec dotenv -e .env -- release-it",
    "release:patch": "pnpm exec dotenv -e .env -- release-it patch",
    "release:minor": "pnpm exec dotenv -e .env -- release-it minor",
    "release:major": "pnpm exec dotenv -e .env -- release-it major",
    "release:dry": "pnpm exec dotenv -e .env -- release-it --dry-run",
    "release:ci": "pnpm exec dotenv -e .env -- release-it --ci",
    "release:ci:patch": "pnpm exec dotenv -e .env -- release-it patch --ci",
    "release:ci:minor": "pnpm exec dotenv -e .env -- release-it minor --ci",
    "release:ci:major": "pnpm exec dotenv -e .env -- release-it major --ci",
    "editor": "claude --dangerously-skip-permissions --model=opus --ide prompt='Before starting the conversation, please read about the project and look around in the codebase and learn the testing '"
  },
  "keywords": [
    "eslint",
    "eslint-config",
    "typescript",
    "javascript",
    "react",
    "preact",
    "tsx"
  ],
  "author": "tupe12334",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tupe12334/eslint-config.git"
  },
  "bugs": {
    "url": "https://github.com/tupe12334/eslint-config/issues"
  },
  "homepage": "https://github.com/tupe12334/eslint-config#readme",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "packageManager": "pnpm@10.18.2",
  "peerDependencies": {
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^9.34.0",
    "@typescript-eslint/eslint-plugin": "^8.40.0",
    "@typescript-eslint/parser": "^8.40.0",
    "eslint": "^9.34.0",
    "eslint-plugin-class-export": "^1.0.1",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-n": "^17.21.3",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-security": "^3.0.1",
    "eslint-plugin-storybook": "^9.1.5",
    "globals": "^16.3.0",
    "typescript-eslint": "^8.40.0"
  },
  "peerDependenciesMeta": {
    "eslint-plugin-preact": {
      "optional": true
    }
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^9.34.0",
    "@release-it/conventional-changelog": "^10.0.1",
    "@typescript-eslint/eslint-plugin": "^8.40.0",
    "@typescript-eslint/parser": "^8.40.0",
    "@typescript-eslint/rule-tester": "^8.44.1",
    "dotenv-cli": "^10.0.0",
    "eslint": "^9.34.0",
    "eslint-plugin-class-export": "^1.0.1",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-n": "^17.21.3",
    "eslint-plugin-preact": "^0.1.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-security": "^3.0.1",
    "eslint-plugin-storybook": "^9.1.5",
    "globals": "^16.3.0",
    "release-it": "^19.0.4",
    "typescript-eslint": "^8.40.0"
  },
  "dependencies": {
    "eslint-plugin-ddd": "^0.5.0",
    "eslint-plugin-default": "^1.1.0",
    "eslint-plugin-error": "^1.1.3",
    "eslint-plugin-guard-clauses": "^0.0.1",
    "eslint-plugin-no-optional-chaining": "^1.0.0",
    "eslint-plugin-required-exports": "^0.2.0",
    "eslint-plugin-single-export": "^1.1.2"
  }
}
